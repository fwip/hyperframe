{
  "name": "hyperframe",
  "productName": "hyperframe",
  "version": "1.0.0",
  "description": "a-frame + hypermerge",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Jemma Nelson",
    "email": "pink.fwip@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "hyperframe"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "aframe": "^1.0.4",
    "better-sqlite3": "^6.0.1",
    "deep-diff": "^1.0.2",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.50",
    "electron": "13.6.6",
    "electron-rebuild": "^1.10.1",
    "hypermerge": "https://github.com/automerge/hypermerge.git",
    "hyperswarm": "^2.10.1",
    "require-rebuild": "^1.2.8"
  },
  "resolutions": {
    "sodium-native": "3.0.1"
  }
}
